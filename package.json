{
  "name": "nest9-template",
  "version": "1.0.0",
  "description": "",
  "author": {
    "name": "test",
    "email": "email@email.com",
    "url": "https://github.com/"
  },
  "private": true,
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/"
  },
  "bugs": {
    "url": "https://github.com/issues"
  },
  "homepage": "https://docs.nestjs.com/",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=build nest build --webpack",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=production node dist/main",
    "start:nest": "nest start",
    "start:dev": "cross-env NODE_ENV=dev nest start --webpack --watch",
    "start:debug": "cross-env NODE_ENV=dev nest start --webpack --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "pre-commit": "lint-staged",
    "test": "jest --runInBand --detectOpenHandles --forceExit --coverage",
    "test:husky": "jest --forceExit --coverage",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "postinstall": "husky install",
    "release": "standard-version",
    "typeorm": "cross-env NODE_ENV=dev typeorm-ts-node-commonjs",
    "migrate:generate": "cross-env NODE_ENV=dev typeorm-ts-node-commonjs migration:generate -d ./src/database/postgres/data-source.ts",
    "migrate:run": "cross-env NODE_ENV=dev typeorm-ts-node-commonjs migration:run -d ./src/database/postgres/data-source.ts",
    "migrate:revert": "cross-env NODE_ENV=dev typeorm-ts-node-commonjs migration:revert -d ./src/database/postgres/data-source.ts"
  },
  "prettier": {
    "semi": true,
    "tabWidth": 2,
    "singleQuote": true,
    "printWidth": 100,
    "trailingComma": "all"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{ts,json,md}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": "^18.x",
    "npm": "^6.x",
    "yarn": "1.X"
  },
  "dependencies": {
    "@nestjs/class-transformer": "^0.4.0",
    "@nestjs/class-validator": "^0.13.3",
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/typeorm": "^9.0.1",
    "@types/passport-jwt": "^3.0.9",
    "aws-sdk": "^2.1476.0",
    "bcrypt": "^5.1.0",
    "cheerio": "^1.0.0-rc.12",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "date-fns": "^2.30.0",
    "express": "^4.18.2",
    "firebase": "^10.7.2",
    "firebase-tools": "^13.0.3",
    "helmet": "^6.0.1",
    "joi": "^17.9.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.10.0",
    "prom-client": "^14.0.1",
    "puppeteer": "^21.1.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.4.1",
    "rxjs": "^7.8.0",
    "stripe": "^12.18.0",
    "swagger-stats": "^0.99.2",
    "swagger-ui-express": "^4.6.2",
    "taxjar": "^4.0.1",
    "typeorm": "^0.3.19"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.0",
    "@types/multer": "^1.4.7",
    "@types/node": "18.15.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.3",
    "jest": "29.5.0",
    "lint-staged": "^13.2.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "standard-version": "^9.5.0",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.9.5"
  }
}
